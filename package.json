{
  "name": "tessla2",
  "main": "./lib/tessla",
  "version": "1.0.3",
  "description": "This packages provides some IDE-like functions for C-Code and TeSSLa specifications",
  "keywords": [
    "tessla",
    "tessla-ide",
    "ide",
    "atom",
    "instrumenting",
    "verification",
    "runtime",
    "C",
    "C-Code"
  ],
  "activationCommands": {
    "atom-workspace": "tessla2:activate"
  },
  "repository": "https://github.com/malteschmitz/tessla2-atom",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "atom-package-deps": "^4.6.0",
    "dockerode": "^2.5.3",
    "fs": "0.0.1-security",
    "fs-extra": "^3.0.1",
    "os": "^0.1.1",
    "path": "^0.12.7",
    "scan-folder": "^1.0.0"
  },
  "consumedServices": {
    "tool-bar": {
      "versions": {
        "^0 || ^1": "consumeToolBar"
      }
    },
    "flexible-panels": {
      "versions": {
        ">=1.17.0": "consumeFlexiblePanels"
      }
    }
  },
  "package-deps": [
    "tool-bar",
    "flexible-panels"
  ]
}
